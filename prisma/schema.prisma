generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = "mysql://root@localhost:3306/latihan_db_karyawann"
}

model employee {
  id_employee Int    @id @default(autoincrement())
  nama        String @unique(map: "nama") @db.VarChar(255)
  job         String @db.VarChar(255)
  salary      Int
}
